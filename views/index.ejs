        <!--博文内容区域 start-->
        <div class="row">
            <div class="col-md-12">
               <!--循环输出结果,实现新闻列表的显示-->
               <% list.forEach(function(article){ %>  
                    <div class="media">
                        <a class="media-left media-middle" href="#">
                            <img src="<%= article.uhead%>" alt="..." class="img-circle">
                        </a>
                        <div class="media-body">
                            <a href="/news_details?newsid=<%=article.aid%>"><h4 class="media-heading"><%= article.antitle%></h4></a>
                            <p><%= article.adesc%></p>
                        </div>
                    </div>
                <% }) %>
            </div>
        </div>
        <!--博文内容 end-->
        <!--分页栏 start-->
        <div class="fenye">
